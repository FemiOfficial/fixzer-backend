{
  "name": "fixzer",
  "version": "1.0.0",
  "author": "Ogunwole samuel",
  "license": "MIT",
  "private": true,
  "main": "./src/app.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --require babel-register --exit",
    "cover": "nyc --reporter=html --reporter=text mocha --require babel-register --recursive --exit",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && babel ./src -d build --copy-files",
    "start": "node ./build/app.js",
    "start:dev": "./node_modules/.bin/babel-watch app.js",
    "pm2:restart": "./pm2.sh"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "json-request-validator": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongodb": "^3.2.3",
    "mongoose": "^5.5.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "request": "^2.88.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "7.1.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.3.0",
    "regenerator-runtime": "0.12.1",
    "request-promise-native": "1.0.5",
    "rimraf": "2.6.2"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "class-methods-use-this": 0,
      "no-underscore-dangle": 0,
      "camelcase": 0,
      "no-restricted-syntax": 0,
      "no-await-in-loop": 0,
      "max-len": 0,
      "no-plusplus": 0,
      "no-continue": 0,
      "guard-for-in": 0,
      "no-param-reassign": 0,
      "no-nested-ternary": 0
    }
  },
  "eslintIgnore": [
    "py/"
  ]
}
